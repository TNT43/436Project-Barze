<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"><![CDATA[


    />

    ]]>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Bar Name"
        android:textColor="@color/quantum_black_text"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/singlebartextview"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="264dp" />

    <TextView
        android:id="@+id/singlebartextview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="308dp"
        android:textColor="#86AD33"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Bar Rating"
        android:textColor="@color/quantum_black_text"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/singlebarratingview"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/singlebarratingview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="144dp"
        android:textColor="#86AD33"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/singlebartextview" />

</androidx.constraintlayout.widget.ConstraintLayout>